<div id="package_form">

<%= simple_form_for @package, validate: true, html: {class: 'form-horizontal'} do |f| %>
  <%= f.error_notification %>
  <%= f.error :base %>

  <div class="pageBlock">

    <div>

      <div id="my-tab-content" class="tab-content">
        <div id="package-main-form" class="tab-pane active">
          <%= render 'packages/form_main', :f => f %>
        </div>
      </div>

    </div>
    
  </div>

<% end %>
</div>
