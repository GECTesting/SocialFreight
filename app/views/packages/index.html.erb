<% @packages.each do |package| %>
  <tr>
    <td><%= package.total %>&nbsp;<%= package.pack_type %></td>
    <td><%= package.imo %></td>
    <td><%= package.gtip %></td>
    <td><%= package.po %></td>
    <td><%= package.brutwg %></td>
    <td><%= package.netwg %></td>
    <td><%= package.volume %></td>
    <td><%= package.ladameter %></td>
    <td><%= package.description %></td>
    <td><%= package.notes %></td>
    <td><%= link_to "Edit", edit_package_path(package) %></td>
    <td><%= link_to 'Destroy', package, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
