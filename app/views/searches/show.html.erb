<% case @search.model %>
  <% when "positions" %>
    <%= render 'searches/position_form' %>
    <%= render 'positions/positions', :positions => @search.positions %>
  <% when "loadings" %>
    <%= render 'searches/loading_form' %>
    <%= render 'loadings/loadings', :loadings => @search.loadings %>
  <% when "reservations" %>
    <%= render 'searches/reservation_form' %>
    <%= render 'reservations', :reservations => @search.reservations %>
<% end %>
