<div class="profile-content">
  <table class="outer" width="100%" id="contentTable" border="0" cellspacing="0" cellpadding="0">
    <tr>
      <td class="oRight" id="bodyCell">
          <div id="inner-content">
            <div id="position" class="profile">
              <ul class="nav nav-pills">
                <li class="active"><a data-toggle="tab" href="#position_profile">Profile</a></li>
                <li><a data-toggle="tab" href="#position_detail">Details</a></li>
                <li><a data-toggle="tab" href="#position_docs">Documents</a></li>
                <li style="float:right;padding-right:10px;">
        <div class="btn-group">
          <a class="btn btn-success" href="#" data-toggle="dropdown"><i class="icon-plus-sign icon-white"></i>Taşıma Bilgisi Ekle</a> 
          <a class="btn btn-success dropdown-toggle" href="#" data-toggle="dropdown">
            <span class="caret" />
          </a>
          <ul class="dropdown-menu">
            <li><%= link_to "Gemi/RORO Taşıma Bilgisi Ekle", new_transport_path(position_id: @position.id, :trans_method => "sea") %></li>
            <li><%= link_to "Tır Taşıma Bilgisi Ekle", new_transport_path(position_id: @position.id, :trans_method => "road") %></li>
            <li><%= link_to "Tren Taşıma Bilgisi Ekle", new_transport_path(position_id: @position.id, :trans_method => "rail") %></li>
            <li><%= link_to "Uçak Taşıma Bilgisi Ekle", new_transport_path(position_id: @position.id, :trans_method => "air") %></li>
            <li><%= link_to "Dahili(Ön/Son) Taşıma Bilgisi Ekle", new_transport_path(position_id: @position.id, :trans_method => "inland") %></li>
          </ul>
        </div>
                </li>
              </ul>
              <div id="my-tab-content" class="tab-content">
                <%= render 'tab1', :position => @position %>
                <%= render 'tab2', :position => @position %>
                <%= render 'tab3', :position => @position %>
              </div>
            </div>
            <div id="position_comments" class="feeds">
              <%= render 'comments/commentable', :object => @position %>
            </div>
          </div>
      </td>
      <td id="rightCell" class="sidebarCell">
          <div id="rightbar">
            <%= render 'goods', :position => @position %>
            <%= render 'todos', :position => @position %>
            <%= render 'loadings/followers', :loading => @position %>
          </div>
      </td>
    </tr>
  </table>
  <div class="modal fade" id="loadModalList"></div>
</div>
