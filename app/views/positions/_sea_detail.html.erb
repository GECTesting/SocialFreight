<tr><td colspan="2" class="subHeader"><h5>Sefer Bilgileri</h5></td></tr>
<tr>
  <%= f.input :vessel, :label => I18n.t("position.#{@position.operation}.label.vesselvoyage") do %>
    <%= f.input_field :vessel, :class => "input-small" %>
    <%= f.input_field :voyage, :class => "input-medium" %>
  <% end %>
  <%= f.input :ordino_date, :input_html => { :class => "input-small", "data-datepicker" => "datepicker" } %>
</tr>
<tr>
  <%= f.input :vessel2, :label => I18n.t("position.#{@position.operation}.label.vessel2voyage") do %>
    <%= f.input_field :vessel2, :class => "input-small" %>
    <%= f.input_field :voyage2, :class => "input-medium" %>
  <% end %>
  <%= f.input :sob_date, :input_html => { :class => "input-small", "data-datepicker" => "datepicker" }, :label => "SOB Date" %>
</tr>
