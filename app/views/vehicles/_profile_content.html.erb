<div class="profile-content">

  <div class="row-fluid profile-summary">
    <%= link_to "Kapı Hareketleri(1)", "##{@vehicle.id}_loadingList" %> |
    <%= link_to "Yakıt Hareketleri(2)", "##{@vehicle.id}_documentList" %> |
    <%= link_to "Seferleri(1)", "##{@vehicle.id}_costList" %>
  </div>
  <div class="row-fluid profile-section">
    <%#= render "shared/related_list", list_model: "transroute", parent: @vehicle, objects: @vehicle.transroutes %>
  </div>

  <div class="row-fluid profile-section">
    <%#= render "shared/related_list", list_model: "document", parent: @vehicle, objects: @vehicle.documents %>
  </div>

  <div class="row-fluid profile-section">
    <%#= render "shared/related_list", list_model: "cost", parent: @vehicle, objects: @vehicle.payoffs %>
  </div>

  <div class="row-fluid profile-section">
    <div id="vehicle_comments" class="feeds">
      <%= render 'comments/commentable', :object => @vehicle %>
    </div>
  </div>

</div>