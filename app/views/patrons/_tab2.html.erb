<div id="user_list" class="tab-pane profile-content">
  <div class="row">
    <div id="new_user_btn">
      <%= link_to "Add User", "#", :class => "btn primary togglebutton", "data-placement" => "below", "data-href" => "#new_user_div" %>
    </div>
    <div id="new_user_div" class="row slidingDiv">
      <%= render 'users/patron_user', :user => User.new() %>
    </div>
  </div>

  <div class="row">

    <table id="users_table" class="zebra-striped">
      <%= render patron.users %>
    </table>

  </div>

</div>
