<div id="transroute_form">
<%= simple_form_for @transroute, html: { class: 'form-horizontal' }, validate: true do |f| %>
  <%= f.error_notification %>
  <%= f.error :base %>

  <div class="pageBlock">

    <div id="my-tab-content" class="row-fluid">
      <div class="page-header">
        <h4><%= t("transroutes.title.transroute_info") %></h4>
      </div>
      <div id="transroute-main-form" class="row-fluid">
        <%= render 'form_main', :f => f %>
      </div>
    </div>

  </div>

<% end %>
</div>
