<div class="profile-content">

        <div class="row-fluid profile-summary">
          <%= link_to "Aktarma Noktaları(1)", "##{@transport.id}_loadingList" %> |
          <%= link_to "Dosyalar(2)", "##{@transport.id}_documentList" %> |
          <%= link_to "Masraf Formları(1)", "##{@transport.id}_costList" %>
        </div>
        <div class="row-fluid profile-section">
          <%= render "shared/related_list", list_model: "transroute", parent: @transport, objects: @transport.transroutes %>
        </div>

        <div class="row-fluid profile-section">
          <%#= render "shared/related_list", list_model: "document", parent: @transport, objects: @transport.documents %>
        </div>

        <div class="row-fluid profile-section">
          <%#= render "shared/related_list", list_model: "cost", parent: @transport, objects: @transport.payoffs %>
        </div>

        <div class="row-fluid profile-section">
          <div id="transport_comments" class="feeds">
            <%= render 'comments/commentable', :object => @transport %>
          </div>
        </div>

</div>