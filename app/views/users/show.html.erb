<% content_for :selected_navigation do%>
  <% @selected_navigation = :home %>
<% end %>

<div id="user" class="row">
  <div class="row">
    <%= render 'profile_header' %>
  </div>
  <div class="row">
    <%= render 'profile_content' %>
  </div>
</div>

<script type="text/javascript" charset="utf-8">
$(function() {
  new Highcharts.Chart({
    chart: {
      renderTo: "user_statics"
    },
    title: {
      text: "Daily Statics"
    },
    xAxis: {
      type: "datetime"
    },
    yAxis: {
      title: {
        text: "Numbers"
      }
    },
    series: [{
      pointInterval: <%= 1.day * 1000 %>,
      pointStart: <%= 3.weeks.ago.to_i*1000 %>,
      data: [1,5,3,8,4,6]
    }]
  });
});
</script>
